    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "@types/file-saver": "^2.0.7",
    "@types/jest": "^27.5.2",
    "@types/node": "^16.18.91",
    "@types/react": "^18.2.71",
    "@types/react-dom": "^18.2.22",
    "file-saver": "^2.0.5",
    "framer-motion": "^11.0.24",
    "i18next": "^23.11.2",
    "i18next-browser-languagedetector": "^7.2.1",
    "mdi-material-ui": "^7.1.0",
    "moment": "^2.30.1",
    "plotly.js": "^2.31.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
